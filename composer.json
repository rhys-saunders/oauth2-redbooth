{
    "name": "rhys-saunders/oauth2-redbooth",
    "description": "Redbooth OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "redbooth",
        "redbooth api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rhys Saunders",
            "email": "rhyssaunders90@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "1.*|2.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0",
        "phpunit/phpunit": "5.6"
    },
    "autoload": {
        "psr-4": {
            "RhysSaunders\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RhysSaunders\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}